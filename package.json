{
  "version": "2.17.2",
  "name": "typebot.io",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "workspaces": [
    "packages/*",
    "packages/deprecated/*",
    "packages/embeds/*"
    
  ],
  "scripts": {
    "prepare": "husky install",
    "docker:up": "docker compose -f docker-compose.dev.yml up -d && sleep 5",
    "docker:nuke": "docker compose -f docker-compose.dev.yml down --volumes --remove-orphans",
    "comment": "turbo run lint",
    "lint": "",
    "dev": "yarn  docker:up  && turbo build --filter=@typebot.io/nextjs... && turbo run dev --filter=@typebot.io/builder... --filter=@typebot.io/viewer... --parallel --no-cache",
    "build": "yarn  docker:up && turbo run build",
    "build:apps": "turbo run build --filter=builder... --filter=viewer...",
    "db:migrate": "cd packages/prisma && yarn run db:migrate",
    "generate-change-log": "git fetch --all && pnpx gitmoji-changelog"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "cz-emoji": "1.3.2-canary.2",
    "husky": "^8.0.3",
    "prettier": "2.8.8",
    "turbo": "1.10.12"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-emoji"
    }
  },
  "engines": {
    "node": ">=18.15.0"
  },
  "packageManager": "yarn"
}
